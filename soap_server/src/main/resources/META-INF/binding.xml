<?xml version="1.0"?>
<binding name="binding"
         force-classes="true"
         namespace="http://maribo.by/web_service/entity"
         add-constructors="true">
    <mapping abstract="true" type-name="method" class="by.maribo.web_service.entity.Method" >
        <value style="element" name="id" field="id" usage="optional"/>
        <value style="element" name="name" field="name" usage="optional"/>
        <value style="element" name="description" field="description" usage="optional"/>
        <value style="element" name="necessity" field="necessity" usage="optional"/>
    </mapping>
    <mapping abstract="true" type-name="entity" class="by.maribo.web_service.entity.Entity">
        <value style="element" name="id" field="id" usage="optional"/>
        <value style="element" name="name" field="name" usage="optional"/>
        <value style="element" name="description" field="description" usage="optional"/>
    </mapping>
</binding>